{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "ang2-conduit"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "assets/theme/bootstrap/css/bootstrap.min.css",
        "assets/theme/dist/css/AdminLTE.css",
        "assets/theme/dist/css/skins/skin-purple.min.css",
        "assets/theme/plugins/datepicker/datepicker3.css",
        "assets/theme/plugins/daterangepicker/daterangepicker.css",
        "assets/theme/plugins/colorpicker/bootstrap-colorpicker.min.css",
        "assets/theme/plugins/timepicker/bootstrap-timepicker.min.css",
        "assets/theme/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css",
        "../node_modules/ng-selectize/selectize/selectize.css",
        "../node_modules/jquery-confirm/css/jquery-confirm.css",
        "../node_modules/ng2-slim-loading-bar/style.css",
        "styles.css"
      ],
      "scripts": [
        "assets/theme/plugins/jquery/jquery-2.2.3.min.js",
        "assets/theme/plugins/jQueryUI/jquery-ui.min.js",
        "assets/theme/bootstrap/js/bootstrap.min.js",
        "assets/theme/dist/js/app.min.js",
        "assets/theme/dist/js/demo.js",
        "assets/theme/plugins/slimScroll/jquery.slimscroll.min.js",
        "assets/theme/plugins/ionslider/ion.rangeSlider.min.js",
        "assets/theme/plugins/knob/jquery.knob.js",
        "assets/theme/plugins/morris/morris.min.js",
        "assets/theme/plugins/sparkline/jquery.sparkline.min.js",
        "assets/theme/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js",
        "assets/theme/plugins/jvectormap/jquery-jvectormap-world-mill-en.js",
        "assets/theme/plugins/bootstrap-slider/bootstrap-slider.js",
        "assets/theme/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js",
        "assets/theme/plugins/fastclick/fastclick.js",
        "assets/theme/plugins/datepicker/bootstrap-datepicker.js",
        "assets/theme/plugins/daterangepicker/daterangepicker.js",
        "assets/theme/plugins/colorpicker/bootstrap-colorpicker.min.js",
        "assets/theme/plugins/timepicker/bootstrap-timepicker.min.js",
        "assets/theme/plugins/input-mask/jquery.inputmask.js",
        "assets/theme/plugins/input-mask/jquery.inputmask.date.extensions.js",
        "assets/theme/plugins/input-mask/jquery.inputmask.extensions.js",
        "../node_modules/jquery-confirm/js/jquery-confirm.js",
        
        "../node_modules/moment/min/moment.min.js",
        "../node_modules/moment-timezone/moment-timezone.js",
        "../node_modules/chart.js/dist/chart.min.js",
        "../node_modules/ng-selectize/selectize/selectize.standalone.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
